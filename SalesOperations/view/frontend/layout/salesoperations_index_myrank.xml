<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceContainer name="content">
            <block class="TrainingBackend\SalesOperations\Block\Customer\MyRank" name="myrank" template="TrainingBackend_SalesOperations::myrank.phtml" cacheable="false"/>
        </referenceContainer>
        <referenceBlock name="root">
            <action method="setHeaderTitle">
                <argument translate="true" name="title" xsi:type="string">My Rank</argument>
            </action>
        </referenceBlock>
    </body>
</page>
